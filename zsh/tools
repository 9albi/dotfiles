# vim: filetype=zsh

# starship
eval "$(starship init zsh)"
eval "$(starship completions zsh)"

# zoxide
eval "$(zoxide init zsh)"

# direnv
eval "$(direnv hook zsh)"

# kubectl
eval "$(kubectl completion zsh)"

# atuin
eval "$(atuin init zsh --disable-up-arrow)"
eval "$(atuin gen-completions --shell zsh)"

eval "$(fzf --zsh)"
